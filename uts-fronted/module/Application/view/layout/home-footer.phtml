		$(document).ready(function() {
			$('#loginForm,#registerForm,#forgetPasswordForm,#referFriendForm').formValidation().on('success.form.fv', function(e, data) {	
				e.preventDefault();
			});
		});
		function loginChecking(jForm){ 
			var d 		= new Date();
			var time 	= d.getTime();
			$("#loginForm").formValidation().on('success.form.fv', function(e) {
				e.stopImmediatePropagation();
				var u_email    = $("#useremail").val();
				var u_password = $("#userpassword").val();
				$.ajax({
					type:'POST',
					url:  baseUrl+'checking-login?sRch='+time,
					data:{u_email:u_email,u_password:u_password},
					success: function(data){
						$("#errorLogin").html('');
						if(data.output == "emailnotinrecord"){
							$("#errorLogin").html('Entered email is not in records');
						}else if(data.output == "fail"){	
							$("#errorLogin").html('Please email is requried');					
						}else if(data.output == "wrongdeatils"){
							$("#errorLogin").html('Entered email or password are wrong');	
						}else if(data.output == "success"){
							window.location = baseUrl+'dashboard';
						}
					}
				});
				e.preventDefault();	
			});
		}
		function signUpUser(JFrom){
			$("#registerForm").formValidation().on('success.form.fv', function(e) {
				e.stopImmediatePropagation();
					$.ajax({
						type		:   'POST',
						url			:  	baseUrl+'sign-in/fdata',
						dataType	: 	"json",
						success: function(data){
						}
					});
				
				e.preventDefault();	
			});
		}
		
		
		function forgetForm(){
			$('#forgotpasswordModal').modal('show');
		}
		function closeForget(){
			$('#forgotpasswordModal').formValidation('resetForm', true);
		}
		function CheckForgetEmail(){
			$('#forgetMessage').html('');
			$("#forgetPasswordForm").formValidation().on('success.form.fv', function(e) {
				e.stopImmediatePropagation();
					$.ajax({
						type		:   'POST',
						url			:  	baseUrl+'check-email-exits',
						dataType	: 	"json",
						data:$('#forgetPasswordForm').serialize(),
						success: function(data){
							if(data.output == "success"){
								$('#forgetMessage').html('Successfully Sent Please check your mail.');
							}else{
								$('#forgetMessage').html('Email not exits');
							}
						}
					});
				e.preventDefault();	
			});
		}
		</script>
